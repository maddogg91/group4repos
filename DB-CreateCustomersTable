//Creates the customer table

string sql = "Create table customers (cust_id char (7) primary key, fname varchar2 (14), lname varchar2 (14), phone number(10), email varchar2(30), address varchar2(30), city varchar2(18), state char (2), zip number (5); 

SQLiteCommand command =new SQLiteCommand(sql, m_dbConnection);
command.ExecuteNonQuery();
